<!DOCTYPE html>
<html>
    <head>
        <title><%= @title %></title>
        <meta name="viewport" content="width=device-width" >
        <link media="only screen and (max-device-width:480px)" href="/stylesheets/smartphone.css" type="text/css" rel="stylesheet" />
        <!-- base setting -->
        <%- @loader.js('/javascripts/config/base_setting.js') %>

        <!-- jQuery -->
        <%- @loader.js('/javascripts/lib/jquery.min.js') %>

        <!-- jQuery masonry -->
        <%- @loader.js('/javascripts/lib/jquery.masonry.min.js') %>

        <!-- jQuery masonry -->
        <%- @loader.js('/javascripts/lib/image_loaded.js') %>

        <!-- Bootstrap Core JavaScript -->
        <%- @loader.js('/bootstrap/js/bootstrap.min.js') %>

        <!-- global paramete setting -->
        <% include 'elements/global_setting' %>

        <!-- base css -->
        <% include 'elements/base_css' %>

        <%- @loader.css('/stylesheets/preregistration.css') %>
    </head>
    <body>
        <!-- header -->
        <% include 'elements/preregistration_header' %>
        <% content %>
        <!-- footer -->
        <% include 'elements/footer' %>
         <!-- footer
        <% include 'elements/facebook' %>
        -->
        <!-- Facebook JavaScript SDK -->
        <%- @loader.js('/javascripts/lib/facebook_sdk.js') %>
    </body>
</html>
