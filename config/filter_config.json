{
    "filters": {
        "include_paths": [
            "./lib/filters"
        ],
        "rules": [
            {
                "applies": ["/*"],
                "ignores": ["login", "auth/facebook/*", "preregistration", "/\\.[a-zA-z]*/"],
                "name_space": "authentication_filter.index",
                "as": "authentication"
            }
        ]
    }
}
